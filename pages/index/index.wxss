.search{
  width: 85%;
  border-radius: 15px;
  background-color: #eeee;
  height: 30px;
  margin: 20rpx auto;
  padding-left: 20px;
  /* text-align: center; */
}
.center{
  margin-left: 40px;
  font-size: 12px;
}
.section{
  display: flex;
  flex-direction: roe;
  width: 95%;
  margin: 20rpx auto;
  box-shadow: 1rpx -5rpx 20rpx #b7b1b1;
  justify-content: space-around;
}
.section-item{
  display: flex;
  flex-direction: column;
  text-align: center;
}
.section-item text{
  font-size: 24rpx;
  font-weight: 300;
  color: black;
}
.section-item .iconfont{
  font-size: 58rpx;
}
.section-item:nth-of-type(1) .iconfont{
  color: rgb(88, 198, 254);
}
.section-item:nth-of-type(2) .iconfont{
  color: #8bda33;
}
.section-item:nth-of-type(3) .iconfont{
  color: #e44724;
}
.section-item:nth-of-type(4) .iconfont{
  color: #bbc516;
}
.section-item:nth-of-type(5) .iconfont{
  color: rgb(252, 188, 0);
}
.main{
  width: 95%;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
}
.main-item{
  display: flex;
  flex-direction: row;
}
.hotel{
  background: red;
}
.homestay{
  background: yellow;
}
.traffic{
  background: pink;
}
.hotel,.homestay,.traffic{
  display: flex;
  color: white;
  justify-content: center;
  align-items: center;
  border-right: 1px solid white;
  border-bottom: 1px solid white;
  flex: 0 0 33.33%;
  height: 100px;
}
.main-nav{
  display: flex;
  flex-direction: column;
}
.main-nav-section{
  display: flex;
  flex-direction: row;
  width: 95%;
  justify-content: space-around;
}
/* 超值特惠 */
.Preferential{
  width: 95%;
  margin: 0 auto;
}
.preferential-title{
  text-align: center;
  font-size: 36rpx;
}
.preferential-wrp{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.preferential-item{
  flex: 0 0 33%;
  text-align: center;
}